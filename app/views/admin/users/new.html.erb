<% provide(:title, '选手注册') %>
<h1>选手注册</h1>
<div class="row">
  <div class="large-6 large-centered columns">
    <%= form_for(@user) do |f| %>
      <%= render 'shared/error_msg', object: f.object, name: "表格" %>
      <%= render 'fields', f: f %>
      <div class="row">
	<div class="small-3 large-3 large-centered small-centered columns">
	  <%= f.submit "注册", class: "button", data: { disable_with: '正在处理' } %>
	</div>
      </div>
    <% end %>
  </div>
</div>
