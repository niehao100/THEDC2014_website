<% provide(:title, '创建队伍') %>
<h1>创建队伍！</h1>
<div class="row">
  <div class="large-8 large-centered columns">
    <%= form_for(@team) do |f| %>
      <%= render 'shared/error_msg', object: f.object, name: "表格" %>
      <%= render 'fields', f: f %>
      <div class="row">
	<div class="small-3 large-3 large-offset-4 columns">
	  <%= f.submit "确认", class: "button", data: { disable_with: '正在处理' } %>
	</div>
      </div>
    <% end %>
  </div>
</div>
